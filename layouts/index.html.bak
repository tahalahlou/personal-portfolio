{{ define "main" }}
<head>
    <link rel="stylesheet" type='text/css' href="https://cdn.jsdelivr.net/gh/devicons/devicon@latest/devicon.min.css" />
    <!-- FAVICONS -->
    <link rel="icon" href='{{ "favicon.ico" | relURL }}?v=12' sizes="any" type="image/x-icon">
    <link rel="icon" type="image/png" sizes="32x32" href='{{ "favicon-32x32.png" | relURL }}?v=12'>
    <link rel="icon" type="image/png" sizes="16x16" href='{{ "favicon-16x16.png" | relURL }}?v=12'>
    <link rel="apple-touch-icon" sizes="180x180" href='{{ "apple-touch-icon.png" | relURL }}?v=12'>
    <link rel="manifest" href='{{ "site.webmanifest" | relURL }}?v=12'>

    <!-- Fonts & Devicons -->
    <link href="https://fonts.googleapis.com/css2?family=Space+Grotesk:wght@400;500;600;700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/gh/devicons/devicon@latest/devicon.min.css" />

    <!-- Language switcher minimal CSS -->
    <style>
    .language-switcher{position:relative;display:inline-block}
    .language-switcher-btn{display:flex;align-items:center;gap:.25rem;cursor:pointer}
    .language-dropdown{display:none;position:absolute;background:var(--bg-secondary);min-width:120px;box-shadow:0 8px 16px rgba(0,0,0,.1);z-index:1;border-radius:.5rem;overflow:hidden}
    .language-switcher:hover .language-dropdown{display:block}
    .language-dropdown a{padding:.75rem 1rem;display:block;text-decoration:none;transition:background-color .2s}
    .language-dropdown a:hover{background:rgba(0,0,0,.05)}
    html.dark .language-dropdown a:hover{background:rgba(255,255,255,.05)}
    .language-active{font-weight:600}
    </style>
</head>
<div class="w-full md:w-4/5 lg:w-3/5 mx-auto px-14 py-12">
    <!-- Hero Section -->
    {{ $authorData := index site.Data .Language.Lang "author" }}
    {{ $author := cond (and $authorData $authorData.author) $authorData.author site.Params }}
    
    {{ with $author }}
    <section class="mb-16">
        <div class="max-w-3xl ml-0 p-4 pb-2 border border-border-primary-light dark:border-border-primary-dark rounded-lg bg-bg-secondary-light dark:bg-bg-secondary-dark shadow-lg relative">
            <div class="text-left">
                <h1 class="text-4xl font-bold mb-2 text-text-primary-light dark:text-text-primary-dark">{{ .name }}</h1>
                <p class="text-gray-400 mb-3 flex items-center text-xl">
                    <svg xmlns="http://www.w3.org/2000/svg" class="text-gray-400" width="24" height="24" viewBox="-7 -2 24 24"><path fill="currentColor" d="M4 9.9A5.002 5.002 0 0 1 5 0a5 5 0 0 1 1 9.9V19a1 1 0 0 1-2 0V9.9zM5 8a3 3 0 1 0 0-6a3 3 0 0 0 0 6z"/></svg>
                    {{ .location }}
                </p>
                <p class="text-xl mb-6 text-text-primary-light dark:text-text-primary-dark">{{ .description }}</p>
            </div>
            <!-- Social Icons - Moved to bottom for mobile -->
            <div class="mt-8 md:mt-0 flex flex-wrap justify-center gap-3 md:absolute md:top-4 md:right-4 md:flex-nowrap md:justify-end md:mt-0">
                {{ with .email }}
